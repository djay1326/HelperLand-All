                </div>
                <div class="logoData">
                    <p class="mb-0 text-center">info@helperland.com <br>&nbsp</p>
                </div>
            </div>
        </div>
        <hr>
        <form method="post" asp-controller="Starting" asp-action="ContactAdd">
            <h4 class="text-center Heading font-weight-bold">Get in touch with us</h4>
            <div class="form-row">
                <div class="form-group col-md-6">
                    <input type="text" class="form-control" id="firstName" placeholder="First Name" asp-for="FirstName">
                    <span asp-validation-for="FirstName" style="color:red"></span>
                </div>
                <div class="form-group col-md-6">
                    <input type="text" class="form-control" id="lastName" placeholder="Last Name" asp-for="LastName">
                    <span asp-validation-for="LastName" style="color:red"></span>
                </div>
            </div>
            <div class="form-row">
                <div class="form-group col-md-6">
                    <label class="sr-only" for="inlineFormInputGroup">Mobile Nubmer</label>
                    <div class="input-group mb-2">
                        <div class="input-group-prepend">
                            <div class="input-group-text">+91</div>
                        </div>
                        <input type="tel" class="form-control" id="inlineFormInputGroup" placeholder="Mobile Nubmer" asp-for="MobileNo">
                        <span asp-validation-for="MobileNo" style="color:red"></span>
                    </div>
                </div>
                <div class="form-group col-md-6">
                    <input type="email" class="form-control" id="inputAddress2" placeholder="Email Address" asp-for="Email">
                    <span asp-validation-for="Email" style="color:red"></span>
                </div>
            </div>
            <div class="form-row">
                <div class="form-group col-md-12">
                    <select id="inputState" class="form-control Dropdown-select" asp-for="SubjectType">
                        <option selected>General</option>
                        <option>Inquiry</option>
                        <option>renewal</option>
                        <option>revocation</option>
                    </select>
                </div>
            </div>
            <div class="form-row">
                <div class="form-group col-md-12">
                    <textarea rows="8" cols="30" class="form-control" placeholder="Message" asp-for="Msg"></textarea>
                </div>
            </div>
            <div class="form_btn w-100 mt-3 d-flex align-items-center justify-content-center">
                <button type="submit" class="btn submitBtn rounded-pill text-white">
                    Submit
                </button>
            </div>
        </form>
    </div>
